{
  "name": "flutter-mvvm-architecture",
  "displayName": "Flutter MVVM Architecture",
  "description": "This extension creates a base folder structure as suggested by the MVVM architecture guide by the Flutter team.",
  "version": "1.2.0",
  "publisher": "Maryusz",
  "icon": "assets/circuit-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Maryusz/flutter-mvvm-architecture"
  },
  "bugs": {
    "url": "https://github.com/Maryusz/flutter-mvvm-architecture/issues"
  },
  "homepage": "https://github.com/Maryusz/flutter-mvvm-architecture",
  "license": "MIT",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mvvmFlutterArchitecture.createProjectStructure",
        "title": "MVVM Flutter: Create Project Structure"
      },
      {
        "command": "mvvmFlutterArchitecture.createFeatureInPresentationLayer",
        "title": "MVVM Flutter: Create Feature in Presentation Layer"
      },
      {
        "command": "mvvmFlutterArchitecture.createFeatureInAllLayers",
        "title": "MVVM Flutter: Create Feature in All Layers"
      },
      {
        "command": "mvvmFlutterArchitecture.createBaseConfiguration",
        "title": "MVVM Flutter: Create Base Configuration"
      },
      {
        "command": "mvvmFlutterArchitecture.createBaseRouting",
        "title": "MVVM Flutter: Create Base Routing"
      }

    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
